<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE cables [
  <!ATTLIST cable_set id ID #REQUIRED>
]>
<!-- This is the code for the 1D ASDARAC Model -->
<cables default_cable_set="original">
  <!-- Use cable set tags for each possible set of cable attachments -->  
  <cable_set id="original">
      <cable_ideal name="1" attachment_reference="com">
      <properties>
        <force_min>0</force_min>
        <force_max>310</force_max>
      </properties>
      <attachments>
        <attachment>
          <link>0</link>
          <location>0.025 0 0</location>
        </attachment>
        <attachment>
          <link>1</link>
          <location>.125 0 0</location>
        </attachment>
      </attachments>
    </cable_ideal>
    <cable_ideal name="2" attachment_reference="com">
      <properties>
        <force_min>0</force_min>
        <force_max>310</force_max>
      </properties>
      <attachments>
        <attachment>
          <link>0</link>
          <location>-0.025 0 0</location>
        </attachment>
        <attachment>
          <link>1</link>
          <location>-0.125 0 0</location>
        </attachment>
      </attachments>
    </cable_ideal>
  </cable_set>
</cables>
